{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev 'src/index.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LetsTN/curso-Node-API.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Let√≠cia Teixeira do Nascimento <leticia.teix.nas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LetsTN/curso-Node-API/issues"
  },
  "homepage": "https://github.com/LetsTN/curso-Node-API#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "jest": "^26.0.1",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
